<%= form_tag session_path do %>
    <p>Please login.</p>
    <br>
    <p>
        <label>Email:</label>
        <%= email_field_tag 'email' %>
    </p>

    <p>
        <label>Password:</label>
        <%= password_field_tag 'password' %>
    </p>
    <p><%= submit_tag 'Login' %></p>
    
<% end %>

